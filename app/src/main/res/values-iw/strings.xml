    <string name="description_wakelock_full">התצוגה והמקלדת נשארים בבהירות מלאה. בלחיצה על כפתור ההפעלה, הנעילה תשוחרר על ידי מערכת ההפעלה.</string>
    <string name="label_wakelock_partial">מעבד</string>
    <string name="description_wakelock_partial">המעבד לא יכנס למצב המתנה/חיסכון בסוללה, גם אם לוחצים על כפתור ההפעלה או שהמסך נכבה. אם למכשירכם ישנה בעיה של התעוררות, זו הנעילה שאתם מעוניינים בה.</string>
    <string name="label_wakelock_screen_dim">מסך מעומעם</string>
    <string name="description_wakelock_screen_dim">השארת המסך דלוק, עם אפשרות לעימעום. לחיצה על כפתור ההפעלה, הנעילה תשוחרר על ידי מערכת ההפעלה.</string>
    <string name="label_wakelock_screen_bright">בהירות מסך</string>
    <string name="description_wakelock_screen_bright">בהירות המסך נשארת מלאה. אם לוחצים על כפתור ההפעלה, הנעילה תשוחרר על ידי המערכת.</string>
    <string name="label_wifilock_full">Wi-Fi רגיל</string>
