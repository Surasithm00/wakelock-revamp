<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string name="app_name">Wakelock v3</string>
    <string name="label_settings">设置</string>
    <string name="action_start">开始</string>
    <string name="label_wakelock_full">屏幕和键盘</string>
    <string name="description_wakelock_full">保持显示器和键盘的亮度。如果你按下电源按钮，锁将被系统隐式释放。</string>
    <string name="label_wakelock_partial">处理器</string>
    <string name="description_wakelock_partial">保持 CPU 进入待机 / 低功耗模式，即使按下电源按钮或显示超时。如果您的设备在唤醒时遇到问题，这就是您想要的锁。</string>
    <string name="label_wakelock_screen_dim">屏幕变暗</string>
    <string name="description_wakelock_screen_dim">保持屏幕开着，但要让屏幕变暗。如果你按下电源键，锁会被系统隐式释放。</string>
    <string name="label_wakelock_screen_bright">屏幕变亮</string>
    <string name="description_wakelock_screen_bright">保持屏幕以最高亮度显示。如果你按下电源按钮，锁将被系统隐式释放。</string>
    <string name="label_wifilock_full">Wi-Fi 正常</string>
    <string name="description_wifilock_full">Wi-Fi 将保持活跃并正常运行，性能和损耗将取决于其他因素，例如屏幕是否关闭。</string>
    <string name="label_wifilock_full_perf">Wi-Fi 高性能</string>
    <string name="description_wifilock_full_perf">即使屏幕关闭，Wi-Fi 仍将保持活跃和高性能模式。</string>
    <string name="label_wifilock_scan_only">仅限 Wi-Fi 扫描</string>
    <string name="description_wifilock_scan_only">当设备进入待机状态时，Wi-Fi 将保持活动状态，但只允许扫描。</string>
    <string name="label_notification_channel_status">状态</string>
    <string name="status_text_idle">闲置</string>
    <string name="galaxy_note4_issue_hint">已知修复 Galaxy Note 4 问题</string>
    <string name="label_autostart_on_boot">启动时自动运行</string>
    <string name="description_autostart_on_boot">重启后自动获取配置的锁。</string>
    <string name="label_category_other">其它</string>
    <string name="label_category_general">常规</string>
    <string name="pause_description_manually_resume">您可以通过按下【播放】按钮手动恢复锁。</string>
    <string name="pause_description_call_only">锁会暂停，因为启用了仅在调用期间获取锁的选项。</string>
    <string name="action_pause">暂停</string>
    <string name="description_onboarding_what">这个应用程序允许你手动获取一个“唤醒锁”，它通过阻止组件进入待机状态来影响设备的电源管理。</string>
    <string name="description_onboarding_what_not">此应用程序不会检测或释放其他应用程序的唤醒信号。它很可能不会改善你的电池寿命。</string>
    <string name="description_onboarding_legacy_version">这个APP在Android存活超过六年之后值得被小翻新一下. 如果你不喜欢新的设计你可以换回旧版.</string>
    <string name="action_download_legacy_version">下载旧版</string>
    <string name="description_onboarding_questions">还有更多的问题？请通过邮件联系我！</string>
    <string name="label_privacy_policy">隐私政策</string>
    <string name="description_darken">本 App 的开发者。</string>
